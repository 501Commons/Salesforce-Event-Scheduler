    <formulas>
        <name>getAgeInYears</name>
        <dataType>Number</dataType>
        <expression>FLOOR(({!$Flow.CurrentDate} - {!varContact.Birthdate}) / 365)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>getAppointmentThisMonthSummary</name>
        <dataType>String</dataType>
        <expression>TEXT({!soScheduledAppointment.Date__c}) + &quot; &quot; + {!soScheduledAppointment.Time__c} + &quot;, &quot;</expression>
    </formulas>
    <formulas>
        <name>getEmailSummary</name>
        <dataType>String</dataType>
        <expression>IF (CONTAINS({!Screen_Appointment_Email}, &quot;@&quot;), 
&quot;Email confirmation sent to &quot; &amp; {!Screen_Appointment_Email}, &quot;Email confirmation not sent since no email associated with your account&quot;)</expression>
    </formulas>
    <formulas>
        <name>getIncomingIdLength</name>
        <dataType>Number</dataType>
        <expression>LEN({!varInputContactId})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>getMonthEnd</name>
        <dataType>Date</dataType>
        <expression>IF( MONTH( {!Screen_Appointment_Date} ) = 12,
  DATE( YEAR( {!Screen_Appointment_Date} ), 12, 31 ), 
  DATE( YEAR( {!Screen_Appointment_Date} ), MONTH ( {!Screen_Appointment_Date} ) + 1, 1 ) - 1 )</expression>
    </formulas>
    <formulas>
        <name>getMonthStart</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR({!Screen_Appointment_Date}), MONTH({!Screen_Appointment_Date}), 1)</expression>
    </formulas>
    <formulas>
        <name>getNewAppointmentName</name>
        <dataType>String</dataType>
        <expression>TEXT({!Screen_Appointment_Date})</expression>
    </formulas>
    <formulas>
        <name>getNewAppointmentScheduledName</name>
        <dataType>String</dataType>
        <expression>{!varContact.Name} + &quot; Appointment Scheduled&quot;</expression>
    </formulas>
